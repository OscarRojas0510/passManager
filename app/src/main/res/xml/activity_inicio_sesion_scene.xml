<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <Transition
        app:constraintSetEnd="@id/end"
        app:constraintSetStart="@+id/start"
        app:duration="2003"
        app:autoTransition="animateToEnd">
    </Transition>

    <ConstraintSet android:id="@+id/start">

        <Constraint
            android:id="@+id/tvLogin"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintHorizontal_bias="0.50"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="30dp"
            android:alpha="0"
            app:transitionEasing="cubic(0.56,0,0.7,0.18)" />
        <Constraint
            android:id="@+id/imageView2"
            android:layout_width="180dp"
            android:layout_height="160dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.50"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:transitionEasing="cubic(0,1,0.84,1)" />
        <Constraint
            android:id="@+id/inicio_inputLay_correo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="0dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.50"
            app:layout_constraintTop_toBottomOf="@+id/imageView2"
            android:layout_marginTop="8dp"
            android:alpha="0"
            app:transitionEasing="cubic(0.56,0,0.7,0.18)"
            android:layout_marginStart="0dp" />
        <Constraint
            android:id="@+id/inicio_inputLay_pass"
            android:elevation="5dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/inicio_inputLay_correo"
            android:layout_marginTop="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="0dp"
            android:alpha="0"
            app:transitionEasing="cubic(0.56,0,0.7,0.18)"
            android:layout_marginEnd="0dp" />
        <Constraint
            android:id="@+id/button"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/inicio_inputLay_pass"
            app:layout_constraintHorizontal_bias="0.50"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="10dp"
            android:alpha="0"
            app:transitionEasing="cubic(0.56,0,0.7,0.18)" />
        <Constraint
            android:id="@+id/button2"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/button"
            app:layout_constraintHorizontal_bias="0.50"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="10dp"
            android:alpha="0"
            app:transitionEasing="cubic(0.56,0,0.7,0.18)" />
        <Constraint
            android:id="@+id/button3"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/button2"
            app:layout_constraintHorizontal_bias="0.50"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="10dp"
            android:alpha="0"
            app:transitionEasing="cubic(0.56,0,0.7,0.18)" />
        <Constraint
            android:id="@+id/imageView3"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="180dp"
            android:layout_height="160dp"
            app:layout_constraintTop_toBottomOf="@+id/button2"
            app:layout_constraintHorizontal_bias="0.50"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="10dp"
            app:transitionEasing="cubic(0.56,0,0.7,0.18)"
            android:alpha="0" />
        <Constraint
            android:id="@+id/tvContrasenia"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/button"
            app:layout_constraintHorizontal_bias="0.50"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="40dp"
            android:alpha="0"/>
        <Constraint
            android:id="@+id/tvRegistro"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/button"
            app:layout_constraintHorizontal_bias="0.50"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="50dp"
            android:alpha="0"/>
        <Constraint
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_editor_absoluteY="512dp"
            app:layout_editor_absoluteX="264dp"
            android:alpha="0"/>
        <Constraint
            android:id="@+id/tvRegistro_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_editor_absoluteX="147dp"
            android:alpha="0"
            app:layout_editor_absoluteY="416dp" />
        <Constraint
            android:id="@+id/sRecordar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_editor_absoluteY="603dp"
            app:layout_editor_absoluteX="45dp"
            android:alpha="0"/>
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/tvLogin"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintHorizontal_bias="0.50"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="30dp"
            app:layout_constraintBottom_toTopOf="@+id/imageView2"
            android:layout_marginBottom="6dp" />
        <Constraint
            android:id="@+id/imageView2"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="180dp"
            android:layout_height="160dp"
            app:layout_constraintTop_toBottomOf="@+id/tvLogin"
            app:layout_constraintHorizontal_bias="0.50"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="5dp" />
        <Constraint
            android:id="@+id/inicio_inputLay_correo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/imageView2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="40dp"
            android:layout_marginStart="40dp" />
        <Constraint
            android:id="@+id/inicio_inputLay_pass"
            android:elevation="5dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/inicio_inputLay_correo"
            android:layout_marginTop="8dp"
            android:layout_marginStart="40dp"
            android:layout_marginEnd="40dp" />
        <Constraint
            android:id="@+id/button"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="311dp"
            android:layout_height="50dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/sRecordar"
            android:layout_marginTop="10dp" />
        <Constraint
            android:id="@+id/button2"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/button"
            app:layout_constraintHorizontal_bias="0.50"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="10dp" />
        <Constraint
            android:id="@+id/button3"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/button2"
            app:layout_constraintHorizontal_bias="0.50"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="0dp" />
        <Constraint
            android:layout_height="160dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.50"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="180dp"
            app:layout_constraintTop_toBottomOf="@+id/button3"
            android:id="@+id/imageView3" />
        <Constraint
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="180dp"
            android:layout_height="160dp"
            app:layout_constraintTop_toBottomOf="@+id/button3"
            app:layout_constraintHorizontal_bias="0.50"
            app:layout_constraintStart_toStartOf="parent"
            android:id="@+id/imageView3"
            android:layout_marginTop="20dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/button"
            android:id="@+id/tvContrasenia"
            android:layout_marginEnd="10dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/tvRegistro"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tvContrasenia"
            app:layout_constraintVertical_bias="0.072"
            app:layout_editor_absoluteX="107dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/textView"
            app:layout_editor_absoluteY="558dp"
            app:layout_editor_absoluteX="147dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:layout_width="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/tvRegistro"
            android:id="@+id/tvRegistro_1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

        <Constraint
            android:id="@+id/sRecordar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/inicio_inputLay_pass"
            android:layout_marginTop="15dp" />
    </ConstraintSet>

</MotionScene>
